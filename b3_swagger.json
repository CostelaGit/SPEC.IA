          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "$ref": "#/definitions/PaymentsByDateResponseApiModel"
            }
          }
        }
      }
    },
    "/v1/Transactions/MakeCurrentScheduledPayment": {
      "post": {
        "tags": [
          "Transactions"
        ],
        "summary": "Makes the current scheduled payment for a loan. Loan must be in service and have an open period, the first open period is the payment that will be made. As of the current version this funtion is not compatible with loans that use the option where interest is collected based on the payment date rather than the end of the period. Model sent in body.",
        "operationId": "Post_Make_Current_Scheduled_Payment",
        "consumes": [
          "application/json",
          "text/json",
          "application/*+json"
        ],
        "produces": [
          "text/plain",
          "application/json",
          "text/json"
        ],
        "parameters": [
          {
            "in": "body",
            "name": "body",
            "description": "",
            "schema": {
              "$ref": "#/definitions/MakeScheduledPaymentApiModel"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "$ref": "#/definitions/MakeScheduledPaymentResponseApiModel"
            }
          }
        }
      }
    }
