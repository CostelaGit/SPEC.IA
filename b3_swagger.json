    "/v1/Loans/AddLoan": {
      "post": {
        "tags": [
          "Loans"
        ],
        "summary": "Add a single loan to the Bryt system, this is the preferred way to add loans.  The call is synchronous and will return success/error information",
        "operationId": "Post_Loan",
        "consumes": [
          "application/json",
          "text/json",
          "application/*+json"
        ],
        "produces": [
          "text/plain",
          "application/json",
          "text/json"
        ],
        "parameters": [
          {
            "in": "body",
            "name": "body",
            "description": "A loan object as specified by the AddLoanApiModel schema",
            "schema": {
              "$ref": "#/definitions/AddLoanApiModel"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "$ref": "#/definitions/AddLoansReponseApiModel"
            }
          }
        }
      }
    },
    "/v1/Loans/AddLoans": {
      "post": {
        "tags": [
          "Loans"
        ],
        "summary": "Adds a loan(s) to the Bryt system exluding twice monthly and single period loans.  It takes an array of loans but it is bettter to just send one loan in each array and make separate calls for each loan.",
        "operationId": "Post_Loans",
        "consumes": [
          "application/json",
          "text/json",
          "application/*+json"
        ],
        "produces": [
          "text/plain",
          "application/json",
          "text/json"
        ],
        "parameters": [
          {
            "in": "body",
            "name": "body",
            "description": "An array of loan objects as specified by the AddLoanApiModel schema",
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/AddLoanApiModel"
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "$ref": "#/definitions/AddLoansReponseApiModel"
            }
          }
        }
      }
    },
    "/v1/Loans/GetLoan": {
      "get": {
        "tags": [
          "Loans"
        ],
        "summary": "Gets a loan by its loan name in Bryt",
        "operationId": "Get_Loan_By_Name",
