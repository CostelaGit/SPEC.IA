          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "$ref": "#/definitions/UdfsReponseApiModel"
            }
          }
        }
      }
    },
    "/v1/Transactions/PaymentsForDate": {
      "get": {
        "tags": [
          "Transactions"
        ],
        "summary": "Returns a list of payments for a given date. This data may change for a given date in the case of Ach payments, the data returned is 'as of now'.",
        "operationId": "Get_Payments_For_Date",
        "produces": [
          "text/plain",
          "application/json",
          "text/json"
        ],
        "parameters": [
          {
            "in": "query",
