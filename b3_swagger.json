    "/v1/Loans/GetLoans": {
      "get": {
        "tags": [
          "Loans"
        ],
        "summary": "Returns basic details of  in service loans, use offset and limit to page requests if you are requesting more that 100 loans.",
        "operationId": "Get_Loans",
        "produces": [
          "text/plain",
          "application/json",
          "text/json"
        ],
        "parameters": [
          {
            "in": "query",
            "name": "offset",
            "description": "Optional parameter indicating the number of loans skipped based on the primary key",
            "type": "integer",
            "format": "int32"
          },
          {
            "in": "query",
            "name": "limit",
            "description": "Maximum number of loans to return, required if 'offset' is used (optional).",
            "type": "integer",
            "format": "int32"
          },
          {
            "in": "query",
            "name": "includePaidOffLoans",
            "description": "Optional parameter that defaults to 'false'",
            "type": "boolean",
            "default": false
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "$ref": "#/definitions/GetLoansApiResponseModel"
            }
          }
        }
      }
    },
    "/v1/Loans/GetLoansByContactId": {
      "get": {
        "tags": [
          "Loans"
        ],
        "summary": "Returns basic details of  in service loans with respect to the contact id passed in.",
