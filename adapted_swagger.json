{
  "info": {
    "title": "My Local API",
    "version": "1.0.0"
  },
  "paths": {
    "/users": {
      "get": {
        "summary": "Get users",
        "responses": {
          "200": {
            "description": "A list of users"
          }
        }
      }
    },
    "/v1/market-data/quotes": {
      "get": {
        "summary": "Get stock quotes",
        "responses": {
          "200": {
            "description": "A list of stock quotes"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "ContactApiModel": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "firstName": {
            "type": "string"
          },
          "lastName": {
            "type": "string"
          },
          "email": {
            "type": "string"
          },
          "phone": {
            "type": "string"
          },
          "brytId": {
            "type": "integer",
            "format": "int32"
          },
          "externalId": {
            "type": "string"
          },
          "addresses": {
            "type": "array",
            "items": {
              "$ref": "#/definitions/AddressApiModel"
            }
          }
        }
      }
    },
    "securitySchemes": {
      "api_key": {
        "type": "apiKey",
        "name": "api_key",
        "in": "header"
      }
    }
  },
  "openapi": "3.0.0"
}